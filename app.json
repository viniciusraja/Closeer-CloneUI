{
  "expo": {
    "name": "Closeer",
    "slug": "closeer-clone-ui",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./src/assets/images/logo-closeer-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#00ff00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "ambev.experience",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "ambev.experience",
      "versionCode": 1
    },
    "description": "",
    "sdkVersion": "38.0.0",
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}